#root {
    flex-direction: column;
    margin: 0 auto;
    max-width: 518px;
    width: 100%;
    height: 100%;
    flex-grow: 1;
    padding: 5px;
    align-items: center;
}

#banner {
    background-image: resource("SDK_Panel_Banner");
    -unity-background-scale-mode: scale-and-crop;
    max-width: 512px;
    width: 100%;
    max-height: 100%;
    height: 85px;
    margin-bottom: 2px;
    flex-shrink: 0;
}

